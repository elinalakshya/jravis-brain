[agent]
expertMode = true
# -----------------------
# .replit config for JRAVIS
# -----------------------

run = "python3.10 main.py"

language = "python3"

# Use Nix to manage dependencies
[packager]
  packageManager = "pip"

[env]
  PORT = "8080"
  JARVIS_SECRET = "MY OG"
  OPENAI_API_KEY = ""
  VA_EMAIL = ""
  VA_EMAIL_PASS = ""

# Set unit test runner to avoid parse errors
[unitTest]
  language = "python3"
